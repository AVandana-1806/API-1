import gov.ca.cwds.rest.api.domain.cms.Address
import gov.ca.cwds.data.access.dto.ReferralEntityAwareDTO

global java.util.Set validationDetailsList

rule "R-07993"
  dialect "mvel"
  agenda-group "referrals-address-data-processing-agenda"
  when
      $address: Address(state == (short)1838, city != null)
  then
      $address.setGovernmentEntityCd((short)1073);
      modify($address);   
end